<%- include('./partials/adminBoilerplate') %>
<title><%=title%></title>
<div class="container">

  <h1>All Users</h1>
  <table class="table">
    <thead class="table-dark">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user => { %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
         <td><a href="/user/<%= user.user_id %>/delete">Delete</a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>
